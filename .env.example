# Gemini API Key for Vision-to-Code
GEMINI_API_KEY=your_gemini_api_key_here

# MongoDB Connection String
MONGODB_URI=mongodb+srv://<username>:<password>@cluster.mongodb.net/flowboard?retryWrites=true&w=majority

# GitHub OAuth (for GitHub integration)
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
NEXT_PUBLIC_GITHUB_CLIENT_ID=your_github_client_id

# Vercel API Token for instant deployment
VERCEL_API_TOKEN=your_vercel_api_token_here
